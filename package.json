{
	"name": "setup-qshell",
	"version": "2.0.0",
	"private": true,
	"description": "setup node action",
	"main": "lib/setup-qshell.js",
	"scripts": {
		"build": "npm i && tsc",
		"format": "prettier --write **/*.ts",
		"format-check": "prettier --check **/*.ts",
		"test": "jest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/foxundermoon/setup-qshell.git"
	},
	"keywords": [
		"actions",
		"qshell",
		"setup"
	],
	"author": "GitHub",
	"license": "MIT",
	"dependencies": {
		"@actions/core": "1.4.0",
		"@actions/github": "^1.0.0",
		"@actions/tool-cache": "1.7.1",
		"@actions/io": "1.1.1"
	},
	"devDependencies": {
		"@types/jest": "^24.0.13",
		"@types/node": "^12.12.14",
		"@types/semver": "^6.0.0",
		"husky": "^2.3.0",
		"jest": "^24.8.0",
		"jest-circus": "^24.7.1",
		"prettier": "1.19.1",
		"ts-jest": "^24.0.2",
		"typescript": "^3.5.1"
	},
	"husky": {
		"skipCI": true,
		"hooks": {
			"pre-commit": "npm run build && npm run format && git add lib",
			"post-commit": "npm prune --production && git add node_modules/* && git commit -m \"Husky commit correct node modules\""
		}
	}
}